@{
    ViewData["Title"] = "登录";
}
<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <script src="~/js/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" src="~/js/account.js"></script>
</head>
<body>
    <div class="col-sm-4 col-form-label">
            <label class="form-label col-form-label">Username</label>
            <input type="text" class="form-control" id="username" placeholder="Enter your username">
    </div>

    <div class="col-sm-4 col-form-label">
        <label class="form-label">Password</label>
        <input type="password" class="form-control" id="pw1" onchange="check1()" placeholder="Enter your password">
        <div class="invalid-feedback">
                Invalid password!
        </div>
    </div>

     <div class="form-group">
        <button type="submit" value="Create" id="login" onclick="login()" class="btn btn-outline-primary">Submit</button>
     </div>
</body>
</html>